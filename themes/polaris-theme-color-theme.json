{
  "name": "Polaris Dark++",
  "type": "dark",
  "colors": {
    "focusBorder": "#5C6AC4",
    "selection.background": "#EBF5FA",
    "dropdown.background": "#43467F",
    "errorForeground": "#f45f42",
    "editor.background": "#00044C",
    "editor.foreground": "#E0F5F5",
    "editorGroup.background": "#43467F",
    "editorGroupHeader.tabsBackground": "#1C2260",
    "tab.inactiveBackground": "#1C2260",
    "activityBar.background": "#00044C",
    "activityBarBadge.background": "#5C6AC4",
    "sideBarTitle.foreground": "#bbbbbb",
    "sideBar.background": "#1C2260",
    "button.background": "#00848E",
    "button.hoverBackground": "#47C1BF",
    "input.placeholderForeground": "#919EAB",
    "input.foreground": "#212B35",
    "input.background": "#B3BCF5",
    "badge.background": "#B3BCF5",
    "badge.foreground": "#F4F6F8",
    "list.hoverBackground": "#5C6AC4",
    "list.focusForeground": "#B3BCF5",
    "list.activeSelectionBackground": "#230051",
    "list.inactiveSelectionBackground": "#1C2260",
    "statusBar.background": "#43467F",
    "statusBar.foreground": "#F4F6F8",
    "statusBar.border": "#1C2260"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#5C6AC4"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#ce7a46"
      }
    },
    {
      "name": "Colors",
      "scope": [
        "constant.other.color"
      ],
      "settings": {
        "foreground": "#5d3594"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#f45f42"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#EEC200"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "constant.other.color",
        "punctuation",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#62be90"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#d3bcf5"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "fontStyle": "italic bold",
        "foreground": "#EEC200"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": [
        "meta.block variable.other",
        "variable"
      ],
      "settings": {
        "foreground": "#00848E"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": [
        "support.other.variable",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#B4E1FA"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "variable.parameter",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#FEAD9A"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#a4dfdf"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "support.type",
        "support.class",
        "support.orther.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types"
      ],
      "settings": {
        "foreground": "#b35747"
      }
    },
    {
      "name": "Entity Types",
      "scope": [
        "support.type"
      ],
      "settings": {
        "foreground": "#EEC200"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#d6c66c"
      }
    },
    {
      "name": "Sub-methods",
      "scope": [
        "entity.name.module.js",
        "variable.import.parameter.js",
        "variable.other.class.js"
      ],
      "settings": {
        "foreground": "#FFC58B"
      }
    },
    {
      "name": "Language methods",
      "scope": [
        "variable.language"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#9C6ADE"
      }
    },
    {
      "name": "entity.name.method.js",
      "scope": [
        "entity.name.method.js"
